{
  "name": "cypress-cucumber-webpack-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^1.6.2",
    "@types/node": "^10.17.27",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0"
  },
  "scripts": {
    "test": "cypress run --spec \"**/*.feature\"",
    "app-test": "cypress run  --env allure=true --spec \"**/*eb*.feature\"",
    "app-test-2": "cypress run  --env allure=true --browser chrome --spec \"**/*eb*.feature\"",
    "test:all": "cypress run --spec \"**/*.features\""
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.5",
    "cypress": "^4.10.0",
    "cypress-cucumber-preprocessor": "latest",
    "ts-loader": "^5.3.1",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0"
  }
}
